{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/generate-case.js": {
      "maxDuration": 60
    },
    "api/expand-case.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}